<% provide(:title, @alumnus.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="alumnus_info">
      <h1>
        <%= @alumnus.name %>
        <%= @alumnus.email %>
        <%= @alumnus.phone %>
        <%= @alumnus.location %>
        <%= @alumnus.college %>
        <%= @alumnus.yale_degree %>
        <%= @alumnus.yale_degree_year %>
        <%= @alumnus.other_degrees %>
        <%= @alumnus.linkedin %>
        <%= @alumnus.employer %>
        <%= @alumnus.employed_field %>
        <%= @alumnus.recommender %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <% @alumnus.attendance.each do |attendance| %>
      <%= link_to attendance.title, attendance_path(attendance) %>
    <% end %>
  </div>
</div>
